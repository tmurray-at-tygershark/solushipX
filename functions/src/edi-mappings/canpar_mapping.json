    "ignoreRowRules": [
        {
            "ruleDescription": "Ignore rows where essential fields like Tracking Number (Barcode) AND Total Charge are both empty",
            "conditions": [
                {
                    "csvHeader": "Barcode",
                    "operator": "isEmpty"
                },
                {
                    "csvHeader": "Total Charge",
                    "operator": "isEmpty"
                }
            ]
        },
        {
            "ruleDescription": "Ignore rows that might be headers repeated in the data (e.g., Barcode cell contains \"Barcode\")",
            "conditions": [
                {
                    "csvHeader": "Barcode",
                    "operator": "equals",
                    "value": "Barcode",
                    "dataType": "string"
                },
                {
                    "csvHeader": "Total Charge",
                    "operator": "equals",
                    "value": "Total Charge",
                    "dataType": "string"
                }
            ]
        },
        {
            "ruleDescription": "Ignore summary lines that contain 'TOTAL' in 'Account Num' and Barcode is empty",
            "conditions": [
                {
                    "csvHeader": "Barcode",
                    "operator": "isEmpty"
                },
                {
                    "csvHeader": "Account Num",
                    "operator": "contains",
                    "value": "TOTAL",
                    "dataType": "string"
                }
            ]
        },
        {
            "ruleDescription": "Ignore summary lines that contain \"Subtotal\" in 'Account Num' and Barcode is empty",
            "conditions": [
                {
                    "csvHeader": "Barcode",
                    "operator": "isEmpty"
                },
                {
                    "csvHeader": "Account Num",
